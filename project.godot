config_version=5

[application]

config/name="kohoma"
config/version="1.0a"
run/main_scene="res://boot/root.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0.168627, 0.168627, 0.168627, 1)
config/icon="res://raw_assets/archive/icon.svg"

[aseprite_importers]

aseprite_executable_path="C:/Program Files/Aseprite/Aseprite.exe"

[autoload]

save_file_auto="*res://save_files/save_file.tscn"
world_auto="*res://world_loader/level_trees/structure/world_autoload/world.tscn"
app_manager_auto="*res://app_manager/app_manager.tscn"
player_loader_auto="*res://player_load/player_loader/player_loader.tscn"
music_player_auto="*res://music_player/autoload/music_player.tscn"
camera_loader_auto="*res://camera_loader/autoload/camera_loader.tscn"
level_border_auto="*res://level_border/level_border.tscn"
stat_bar_auto="*res://ui/stat_bar/stat_bar.tscn"
game_loader_auto="*res://boot/game_over/game_loader.tscn"
sfx_player_auto="*res://sfx_player/autoload/sfx_player.tscn"
dialogue_loader_auto="*res://ui/dialogue_loader/dialogue_loader.tscn"
pause_loader_auto="*res://ui/pause_menu/autoloads/pause_loader.tscn"
file_tester_auto="*res://debugging/save_files/file_tester.tscn"
dir_auto="*res://player_load/enums/Direction.gd"
curtain_module_auto="*res://ui/fade_module/curtain_module.tscn"
debug_helper_auto="*res://debugging/debug_helper.tscn"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=176
window/size/viewport_height=128
window/size/resizable=false
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/Todo_Manager/plugin.cfg", "res://addons/nklbdev.aseprite_importers/plugin.cfg")

